        "takaaki-mizuno/swagger-parser": "0.*",
        "symfony/yaml": "~3.3",
        "friendsofphp/php-cs-fixer": "^2.8",
        "fzaninotto/faker": "~1.5",
        "laravel/laravel": "5.5.*",
        "doctrine/dbal": "~2.5"
    },
    "autoload": {
        "psr-4": {
