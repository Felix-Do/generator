    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "icanboogie/inflector": "^1.4",
        "laravel/framework": "5.5.*",
        "laravel-rocket/foundation": "2.0.*",
        "takaaki-mizuno/php-code-manipulator": "0.*",
